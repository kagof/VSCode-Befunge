{
    "name": "befunge",
    "displayName": "Befunge Syntax Highlighting",
    "description": "Adds syntax highlighting for Befunge-93 and Befunge-98 programming languages",
    "version": "1.0.0",
    "publisher": "kagof",
    "icon": "assets/icon.png",
    "license": "SEE LICENSE IN LICENSE.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/kagof/VSCode-Befunge"
    },
    "homepage": "https://github.com/kagof/VSCode-Befunge",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Languages"
    ],
    "keywords": [
        "befunge",
        "esoteric",
        "befunge-93",
        "befunge-98",
        "funge-98"
    ],
    "bugs": {
        "url": "https://github.com/kagof/VSCode-Befunge/issues"
    },
    "contributes": {
        "languages": [{
            "id": "befunge",
            "aliases": ["Befunge", "befunge", "Befunge-93"],
            "extensions": [".bf",".befunge", ".b93"],
            "configuration": "./befunge-configuration.json"
        },
        {
            "id": "befunge98",
            "aliases": ["Befunge98", "befunge98", "Befunge-98", "funge-98"],
            "extensions": [".b98"],
            "configuration": "./befunge-configuration.json"
        }],
        "grammars": [{
            "language": "befunge",
            "scopeName": "source.befunge.93",
            "path": "./syntaxes/befunge-93.tmLanguage.json"
        },
        {
            "language": "befunge98",
            "scopeName": "source.befunge.98",
            "path": "./syntaxes/befunge-98.tmLanguage.json"
        }]
    }
}